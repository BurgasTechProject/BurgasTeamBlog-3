@model List<Blog.Models.Category>
@{
    ViewBag.Title = "ListCategories";
}

<div class="container">
    
        @foreach (var category in Model)
        {
          <h1 class="col-sm-12 text-center"> @Html.ActionLink(string.Format($"{category.Name}"), "ListArticlesOfCategory", "Category", new { @id = category.Id }, null)</h1>

        }

</div>

